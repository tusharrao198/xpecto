    <div class="hero" style="background-image: url('https://user-images.githubusercontent.com/56690827/162190335-422b76d8-99a5-4fca-97e9-5a22d3154800.png");">
      <div>
          

           
        .<br>
        <br>
        <br>
        <br>
        <br>
           
      </div>
      <div class="container">
        <div class="p-4 d-flex justify-content-center flex-nowrap">
            <h1>
                <a href="/" class="btn btn-primary">
                    <span class="fa fa-user"> Profile </span></a
                >
            </h1>
        </div>
        <div class="card mb-8 mx-auto" style="max-width: 40%">
            <div class="row g-5">
                <div class="col-md-4">
                    <img
                        src="<%= user.image%>"
                        class="img-fluid rounded-start"
                        alt="..."
                    />
                </div>
                <div class="col-md-8">
                    <div class="card-body">
                        <h5 class="card-title"><%= user.displayName %></h5>
                        <p class="card-text">
                            <strong>Id</strong>: <%= user._id %><br />
                            <strong>Email</strong>: <%= user.email %><br />
                            <strong>Name</strong>: <%= user.displayName
                            %><br />
                            <strong>firstName</strong>: <%= user.firstName
                            %><br />
                            <strong>googleid</strong>: <%= user.googleId
                            %><br />
                        </p>
                        <a href="/" class="card-text btn btn-primary">
                            <span class="fa fa-home"></span> Back to Home</a
                        >
                        <a
                            href="/auth/logout"
                            class="card-text btn btn-danger"
                            ><span class="fa fa-google-plus"></span>
                            Logout</a
                        >
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
